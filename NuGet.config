<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <packageSources>
    <clear />
    <add key="nuget.org" value="https://api.nuget.org/v3/index.json" protocolVersion="3" />
    <add key="local" value="http://localhost:5555/v3/index.json" protocolVersion="3" />
    <add key="tools" value="https://pkgs.dev.azure.com/dnceng/public/_packaging/dotnet-tools/nuget/v3/index.json" />
  </packageSources>
  <solution>
    <add key="disableSourceControlIntegration" value="true" />
  </solution>
  <packageSourceMapping>
    <clear />
    <packageSource key="nuget.org">
      <package pattern="*" />
    </packageSource>
    <packageSource key="local">
      <package pattern="Aspire.Hosting.HealthChecksUI" />
    </packageSource>
    <packageSource key="tools">
      <package pattern="Aspire.*" />
    </packageSource>
  </packageSourceMapping>
</configuration>
